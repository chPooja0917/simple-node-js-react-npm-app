pipeline {
    agent { label 'build_java_11' }
    stages {
        stage('source code') {
            steps {
                git branch: '*/master', url: 'https://github.com/chPooja0917/simple-node-js-react-npm-app.git'
            }
            
        }
        stage('install') {
            steps {
                sh 'npm install'
            }
        }
        stage('test code') {
            steps {
                sh 'npm test'
            }
        }
        stage('Build the package') {
            steps {
                sh 'npm run build'
            }
        }
                      
    }

}
